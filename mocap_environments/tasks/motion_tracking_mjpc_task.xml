<mujoco model="Humanoid Track">
  <size memory="5M"/>

  <custom>
    <numeric name="sampling_representation" data="2" />
    <numeric name="agent_planner" data="2" />
    <numeric name="agent_horizon" data="0.75" />
    <numeric name="agent_timestep" data="0.01666666666666667" />
    <numeric name="agent_integrator" data="3" />
    <numeric name="sampling_spline_points" data="8" />
    <numeric name="sampling_exploration" data="0.05" />
    <numeric name="sampling_trajectories" data="16" />
    <numeric name="ilqg_num_rollouts" data="8" />
    <numeric name="ilqg_regularization_type" data="1" />
    <numeric name="ilqg_representation" data="2" />
    <numeric name="residual_Mocap FPS" data="60.0 0.0 240.0" />
  </custom>

  <default>
    <default class="mocap_site">
      <site type="sphere" size="0.027" rgba="0 0 1 1" group="3" material="target"/>
    </default>
  </default>

  <option timestep="0.01666666666666667" integrator="implicitfast"/>

  <sensor>
    <user name="act_dot" dim="21" user="0 0.03 0.0 0.1" />
    <user name="Joint Vel." dim="21" user="0 0.001 0.0 0.01" />
    <user name="Control" dim="21" user="3 0.1 0 1.0 0.3" />

    <user name="Pos[pelvis]" dim="3" user="6 100.0 0.0 100.0 0.1" />
    <user name="Pos[head]" dim="3" user="6 0.0 0.0 100.0 0.1" />
    <user name="Pos[toe]" dim="6" user="6 30.0 0.0 100.0 0.1" />
    <user name="Pos[heel]" dim="6" user="6 30.0 0.0 100.0 0.1" />
    <user name="Pos[knee]" dim="6" user="7 30.0 0.0 100.0 0.2 4" />
    <user name="Pos[hand]" dim="6" user="6 30.0 0.0 100.0 0.1" />
    <user name="Pos[elbow]" dim="6" user="7 30.0 0.0 100.0 0.2 4" />
    <user name="Pos[shoulder]" dim="6" user="6 100.0 0.0 100.0 0.1" />
    <user name="Pos[hip]" dim="6" user="6 30.0 0.0 100.0 0.1" />

    <user name="Vel[root]" dim="3" user="6 0.1 0 1.0 0.3" />
    <user name="Vel[head]" dim="3" user="6 0.0 0 1.0 0.3" />
    <user name="Vel[toe]" dim="6" user="6 0.1 0 1.0 0.3" />
    <user name="Vel[heel]" dim="6" user="6 0.1 0 1.0 0.3" />
    <user name="Vel[knee]" dim="6" user="6 0.1 0 1.0 0.3" />
    <user name="Vel[hand]" dim="6" user="6 0.1 0 1.0 0.3" />
    <user name="Vel[elbow]" dim="6" user="6 0.1 0 1.0 0.3" />
    <user name="Vel[shoulder]" dim="6" user="6 0.1 0 1.0 0.3" />
    <user name="Vel[hip]" dim="6" user="6 0.1 0 1.0 0.3" />

    <framepos name="tracking_pos[pelvis]" objtype="site" objname="walker/tracking[pelvis]" />
    <framepos name="tracking_pos[head]" objtype="site" objname="walker/tracking[head]" />
    <framepos name="tracking_pos[ltoe]" objtype="site" objname="walker/tracking[ltoe]" />
    <framepos name="tracking_pos[rtoe]" objtype="site" objname="walker/tracking[rtoe]" />
    <framepos name="tracking_pos[lheel]" objtype="site" objname="walker/tracking[lheel]" />
    <framepos name="tracking_pos[rheel]" objtype="site" objname="walker/tracking[rheel]" />
    <framepos name="tracking_pos[lknee]" objtype="site" objname="walker/tracking[lknee]" />
    <framepos name="tracking_pos[rknee]" objtype="site" objname="walker/tracking[rknee]" />
    <framepos name="tracking_pos[lhand]" objtype="site" objname="walker/tracking[lhand]" />
    <framepos name="tracking_pos[rhand]" objtype="site" objname="walker/tracking[rhand]" />
    <framepos name="tracking_pos[lelbow]" objtype="site" objname="walker/tracking[lelbow]" />
    <framepos name="tracking_pos[relbow]" objtype="site" objname="walker/tracking[relbow]" />
    <framepos name="tracking_pos[lshoulder]" objtype="site" objname="walker/tracking[lshoulder]" />
    <framepos name="tracking_pos[rshoulder]" objtype="site" objname="walker/tracking[rshoulder]" />
    <framepos name="tracking_pos[lhip]" objtype="site" objname="walker/tracking[lhip]" />
    <framepos name="tracking_pos[rhip]" objtype="site" objname="walker/tracking[rhip]" />

    <framelinvel name="tracking_linvel[pelvis]" objtype="site" objname="walker/tracking[pelvis]" />
    <framelinvel name="tracking_linvel[head]" objtype="site" objname="walker/tracking[head]" />
    <framelinvel name="tracking_linvel[ltoe]" objtype="site" objname="walker/tracking[ltoe]" />
    <framelinvel name="tracking_linvel[rtoe]" objtype="site" objname="walker/tracking[rtoe]" />
    <framelinvel name="tracking_linvel[lheel]" objtype="site" objname="walker/tracking[lheel]" />
    <framelinvel name="tracking_linvel[rheel]" objtype="site" objname="walker/tracking[rheel]" />
    <framelinvel name="tracking_linvel[lknee]" objtype="site" objname="walker/tracking[lknee]" />
    <framelinvel name="tracking_linvel[rknee]" objtype="site" objname="walker/tracking[rknee]" />
    <framelinvel name="tracking_linvel[lhand]" objtype="site" objname="walker/tracking[lhand]" />
    <framelinvel name="tracking_linvel[rhand]" objtype="site" objname="walker/tracking[rhand]" />
    <framelinvel name="tracking_linvel[lelbow]" objtype="site" objname="walker/tracking[lelbow]" />
    <framelinvel name="tracking_linvel[relbow]" objtype="site" objname="walker/tracking[relbow]" />
    <framelinvel name="tracking_linvel[lshoulder]" objtype="site" objname="walker/tracking[lshoulder]" />
    <framelinvel name="tracking_linvel[rshoulder]" objtype="site" objname="walker/tracking[rshoulder]" />
    <framelinvel name="tracking_linvel[lhip]" objtype="site" objname="walker/tracking[lhip]" />
    <framelinvel name="tracking_linvel[rhip]" objtype="site" objname="walker/tracking[rhip]" />
  </sensor>
</mujoco>

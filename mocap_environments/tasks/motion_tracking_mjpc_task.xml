<mujoco model="Humanoid Track">
  <size memory="5M"/>

  <custom>
    <numeric name="agent_planner" data="2" />
    <numeric name="agent_horizon" data="0.6" />
    <numeric name="agent_timestep" data="0.01666666666666667" />
    <numeric name="agent_integrator" data="3" />
    <numeric name="sampling_exploration" data="0.05" />
    <numeric name="sampling_spline_points" data="8" />
    <numeric name="sampling_representation" data="2" />
    <numeric name="sampling_trajectories" data="16" />
    <numeric name="ilqg_num_rollouts" data="8" />
    <numeric name="ilqg_regularization_type" data="1" />
    <numeric name="ilqg_representation" data="2" />
    <numeric name="residual_Mocap FPS" data="60.0 0.0 240.0" />
  </custom>

  <default>
    <default class="mocap_site">
      <site type="sphere" size="0.027" rgba="0 0 1 1" group="2"/>
    </default>
  </default>

  <option timestep="0.01666666666666667" integrator="implicitfast"/>

  <sensor>
    <user name="ctrl" dim="21" user="6 0.00001 0 1.0 0.01" />
    <user name="act_dot" dim="21" user="0 0.003 0.0 0.1" />

    <user name="pos[rt-xy]" dim="2" user="1 10.0 0.0 10.0 0.03 5" />
    <user name="pos[rt-z]" dim="1" user="2 0.0 0.0 10.0 0.1" />
    <user name="pos[head]" dim="3" user="2 0.3 0.0 10.0 0.1" />
    <user name="pos[ltoe]" dim="3" user="2 0.3 0.0 10.0 0.1" />
    <user name="pos[rtoe]" dim="3" user="2 0.3 0.0 10.0 0.1" />
    <user name="pos[lheel]" dim="3" user="1 3.0 0.0 10.0 0.06 5.0" />
    <user name="pos[rheel]" dim="3" user="1 3.0 0.0 10.0 0.06 5.0" />
    <user name="pos[lknee]" dim="3" user="2 0.3 0.0 10.0 0.1" />
    <user name="pos[rknee]" dim="3" user="2 0.3 0.0 10.0 0.1" />
    <user name="pos[lhand]" dim="3" user="1 3.0 0.0 10.0 0.06 5.0" />
    <user name="pos[rhand]" dim="3" user="1 3.0 0.0 10.0 0.06 5.0" />
    <user name="pos[lelbow]" dim="3" user="2 0.3 0.0 10.0 0.1" />
    <user name="pos[relbow]" dim="3" user="2 0.3 0.0 10.0 0.1" />
    <user name="pos[lshldr]" dim="3" user="2 3.0 0.0 10.0 0.06 5.0" />
    <user name="pos[rshldr]" dim="3" user="2 3.0 0.0 10.0 0.06 5.0" />
    <user name="pos[lhip]" dim="3" user="2 0.3 0.0 10.0 0.1" />
    <user name="pos[rhip]" dim="3" user="2 0.3 0.0 10.0 0.1" />

    <user name="vel[rt-xy]" dim="2" user="2 0.3 0.0 10.0 0.3" />
    <user name="vel[rt-z]" dim="1" user="2 0.3 0.0 10.0 0.3" />
    <user name="vel[ltoe]" dim="3" user="2 0.3 0.0 10.0 0.3" />
    <user name="vel[rtoe]" dim="3" user="2 0.3 0.0 10.0 0.3" />
  </sensor>
</mujoco>

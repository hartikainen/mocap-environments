<mujoco model="SMPLHumanoid Track">
  <size memory="3M"/>

  <custom>
    <numeric name="agent_planner" data="2" />
    <numeric name="agent_horizon" data="0.45" />
    <numeric name="agent_timestep" data="0.008333333333" />
    <numeric name="agent_integrator" data="3" />
    <numeric name="sampling_exploration" data="0.05" />
    <numeric name="sampling_spline_points" data="8" />
    <numeric name="sampling_representation" data="1" />
    <numeric name="sampling_trajectories" data="6" />
    <numeric name="ilqg_num_rollouts" data="8" />
    <numeric name="ilqg_regularization_type" data="1" />
    <numeric name="ilqg_representation" data="1" />
    <numeric name="residual_Mocap FPS" data="120.0 0.0 240.0" />
  </custom>

  <default>
    <default class="mocap_site">
      <site type="sphere" size="0.027" rgba="1 0 0 1" group="2"/>
    </default>
  </default>

  <option timestep="0.008333333333" integrator="implicitfast"/>

  <sensor>
    <user name="ctrl" dim="49" user="6 1e-5 0 1.0 0.01" />
    <user name="act_dot" dim="49" user="6 0.003 0 1.0 0.01" />

    <user name="Pos[Pelvis-xy]" dim="2" user="1 3.0 0.0 10.0 0.02 5" />
    <user name="Pos[Pelvis-z]" dim="1" user="2 0.3 0.0 10.0 0.1" />
    <user name="Pos[L_Hip]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[L_Knee]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[L_Ankle]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[L_Toe]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[R_Hip]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[R_Knee]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[R_Ankle]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[R_Toe]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[Torso]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <!-- <user name="Pos[Spine]" dim="3" user="2 0.3 0.0 10.0 0.1" /> -->
    <!-- <user name="Pos[Chest]" dim="3" user="2 0.3 0.0 10.0 0.1" /> -->
    <user name="Pos[Neck]" dim="3" user="2 0.3 0.0 10.0 0.1" />
    <user name="Pos[Head]" dim="3" user="2 0.3 0.0 10.0 0.1" />
    <!-- <user name="Pos[L_Thorax]" dim="3" user="2 0.3 0.0 10.0 0.1" /> -->
    <user name="Pos[L_Shoulder]" dim="3" user="2 3.0 0.0 10.0 0.02" />
    <user name="Pos[L_Elbow]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[L_Wrist]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[L_Hand]" dim="3" user="2 1.0 0.0 10.0 0.1" />
    <!-- <user name="Pos[R_Thorax]" dim="3" user="2 0.3 0.0 10.0 0.1" /> -->
    <user name="Pos[R_Shoulder]" dim="3" user="2 3.0 0.0 10.0 0.02" />
    <user name="Pos[R_Elbow]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[R_Wrist]" dim="3" user="2 1.0 0.0 10.0 0.03" />
    <user name="Pos[R_Hand]" dim="3" user="2 1.0 0.0 10.0 0.1" />
  </sensor>
</mujoco>
